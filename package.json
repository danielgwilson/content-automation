{
  "name": "content-automation",
  "version": "1.0.0",
  "description": "Automated system to (1) scrape Reddit, (2) repackage the content, and (3) upload it to other platforms like YouTube.",
  "main": "index.js",
  "bin": {
    "ryvb": "./lib/index.js"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "oclif": {
    "commands": "./lib/commands",
    "bin": "ryvb",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found"
    ]
  },
  "scripts": {
    "test": "jest -c jest.config.js",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "sh ./scripts/prepack.sh",
    "version": "oclif-dev readme && git add README.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danielgwilson/reddit-youtube-video-bot.git"
  },
  "author": "Daniel G. Wilson",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/danielgwilson/reddit-youtube-video-bot/issues"
  },
  "homepage": "https://github.com/danielgwilson/reddit-youtube-video-bot#readme",
  "private": true,
  "dependencies": {
    "@google-cloud/text-to-speech": "^4.0.2",
    "@nexrender/action-copy": "^1.8.16",
    "@nexrender/action-encode": "^1.8.16",
    "@nexrender/cli": "^1.10.1",
    "@nexrender/core": "^1.39.4",
    "@oclif/command": "^1.6.1",
    "@oclif/config": "^1.15.1",
    "@oclif/plugin-autocomplete": "^0.1.5",
    "@oclif/plugin-help": "^2.2.3",
    "@oclif/plugin-not-found": "^1.2.3",
    "@oclif/test": "^1.2.5",
    "acorn": "^7.2.0",
    "azure-cognitiveservices-websearch": "^2.0.0",
    "bottleneck": "^2.19.5",
    "config": "^3.3.1",
    "cron": "^1.8.2",
    "dotenv": "^16.0.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "ffmpeg-static": "^5.0.2",
    "google-auth-library": "^8.4.0",
    "googleapis": "^107.0.0",
    "jsdom": "^16.2.2",
    "keyword-extractor": "0.0.18",
    "ms-rest-azure": "^3.0.0",
    "node-fetch": "^2.6.1",
    "node-notifier": "^10.0.1",
    "playwright": "^1.25.0",
    "playwright-addons": "github:Niek/playwright-addons#master",
    "progress": "^2.0.3",
    "remove-markdown": "^0.3.0",
    "tslib": "^1.13.0",
    "types-for-adobe": "^1.7.0",
    "underscore.string": "^3.3.5",
    "uuid": "^7.0.3"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@types/config": "0.0.36",
    "@types/cron": "^1.7.2",
    "@types/jest": "^25.2.2",
    "@types/node": "^13.13.6",
    "@types/node-fetch": "^2.5.7",
    "@types/node-notifier": "^5.4.2",
    "@types/prettier": "^1.19.1",
    "@types/progress": "^2.0.5",
    "@types/remove-markdown": "^0.1.1",
    "@types/request": "^2.48.4",
    "@types/underscore.string": "0.0.38",
    "@types/uuid": "^7.0.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "globby": "^11.0.0",
    "prettier": "^1.19.1",
    "ts-node": "^8.10.1",
    "typescript": "^4.7.4"
  },
  "overrides": {
    "@nexrender/action-encode": {
      "ffmpeg-static": "^5.0.2"
    }
  },
  "types": "lib/index.d.ts"
}
